{
  "name": "AmazonExtension!",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Injecting stuff",
	"content_scripts": [
		{
			"matches": ["https://www.amazon.com/*"],
			"js": ["jquery-3.3.1.min.js","inject.js"],
			"run_at": "document_end"
		}
	],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Inject!"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
